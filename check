rows_even = driver.find_elements(By.CLASS_NAME, "react-grid-Row--even")
rows_odd = driver.find_elements(By.CLASS_NAME, "react-grid-Row--odd")

# Combine all rows
all_rows = rows_even + rows_odd

# Extract value attributes from react-grid-Cell within each row
data = []
for row in all_rows:
    cells = row.find_elements(By.CLASS_NAME, "react-grid-Cell")
    row_data = [cell.get_attribute("value") for cell in cells]
